	Select_Dex: .asciiz "\nWhich region would you like to select? (1 for Kanto Region, 2 for Hoenn Region): " 
	Ask_KantoPrompt: .asciiz "\nPlease select a Pokedex Number to show the selected Kanto Pokemon's Pokedex Entry (1-151): "
	Ask_HoennPrompt: .asciiz "\nPlease select a Pokedex Number to show the selected Hoenn Pokemon's Pokedex Entry (1-134): "
	
.text
	#Making array of strings for Kanto Pokedex
	la $t0, my_KantoArray
	
	#Making array of string for Hoenn Region
	la $t3, my_HoennArray
	
	#implementation of pokemon dex entries by giving each entry a space in the array.
	#Kanto Entry Implementation
	la $t4, DEX_BULBASAUR
	sw $t4, 0($t0)
	la $t4, DEX_IVYSAUR
	sw $t4, 4($t0)
	la $t4, DEX_VENUSAUR
	sw $t4, 8($t0)
	la $t4, DEX_CHARMANDER
	sw $t4, 12($t0)
	la $t4, DEX_CHARMELEON
	sw $t4, 16($t0)
	la $t4, DEX_CHARIZARD
	sw $t4, 20($t0)
	la $t4, DEX_SQUIRTLE
	sw $t4, 24($t0)
	la $t4, DEX_WARTORTLE
	sw $t4, 28($t0)
	la $t4, DEX_BLASTOISE
	sw $t4, 32($t0)
	la $t4, DEX_CATERPIE
	sw $t4, 36($t0)
	la $t4, DEX_METAPOD
	sw $t4, 40($t0)
	la $t4, DEX_BUTTERFREE
	sw $t4, 44($t0)
