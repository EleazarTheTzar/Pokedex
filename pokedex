	la $t1, DEX_GROUDON
	sw $t1, 520($t3)
	la $t1, DEX_RAYQUAZA
	sw $t1, 524($t3)
	la $t1, DEX_JIRACHI
	sw $t1, 528($t3)
	la $t1, DEX_DEOXYS
	sw $t1, 532($t3)
	
	#do while loop that repeatedly asks user for dex entries
	do:
	
	#ask which region pokedex they want
	li $v0, 4
	la $a0, Select_Dex
	syscall
	
	#user enters region number
	li $v0, 5
	syscall
	
	# region selection is saved
	move $t5, $v0
	
	#if statement that compares region selection to determine which pool of pokemon to choose from
	beq $t5, 1, Kanto 
	beq $t5, 2, Hoenn
	
	home:
	
	while: #perform until false
	blt $v0, 1, exit
	j do
	
	#exit function
	exit:
	li $v0, 10
	syscall
	
Kanto:
	#ask user to select pokedex number
	li $v0, 4
	la $a0, Ask_KantoPrompt
	syscall
	
	#User enters input
	li $v0, 5
	syscall
	
	#index is saved
	addi $v0, $v0, -1
	move $t3, $v0
	sll $t3, $t3, 2
	
	#printing the indexed element
	li $v0, 4
	la $a1, my_KantoArray
	add $a1, $a1, $t3
	lw $a0, ($a1)
	syscall
	
	j home

Hoenn:
	#ask user to select pokedex number
	li $v0, 4
	la $a0, Ask_HoennPrompt
	syscall
	
	#User enters input
	li $v0, 5
	syscall
	
	#index is saved
	addi $v0, $v0, -1
	move $t3, $v0
	sll $t3, $t3, 2
	
	#printing the indexed element
	li $v0, 4
	la $a1, my_HoennArray
	add $a1, $a1, $t3
	lw $a0, ($a1)
	syscall
	
	j home
